GO
ALTER TABLE [dbo].[clientes]  WITH CHECK ADD  CONSTRAINT [fk_clientes_personas] FOREIGN KEY([cod_persona])
REFERENCES [dbo].[personas] ([cod_persona])
GO
ALTER TABLE [dbo].[clientes] CHECK CONSTRAINT [fk_clientes_personas]
GO
ALTER TABLE [dbo].[clientes]  WITH CHECK ADD  CONSTRAINT [fk_clientes_tipos_clientes] FOREIGN KEY([cod_tipo_cliente])
REFERENCES [dbo].[tipos_clientes] ([cod_tipo_cliente])
GO
ALTER TABLE [dbo].[clientes] CHECK CONSTRAINT [fk_clientes_tipos_clientes]
GO
